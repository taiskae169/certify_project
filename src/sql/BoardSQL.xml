<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="board">
	<select id="getCount" resultType="int">
		select count(*) from board
	</select>
	<select id="getAriticle" parameterType="map" resultType="board.vo.BoardVO">
  		select * from (select num,title,id,content,newname,reg_date,step,orgname,readcount,rownum r from (select * from board order by num desc) order by num desc) 
  		<![CDATA[where r >= #{start} and r <= #{end}]]> 
  	</select>
  	<select id="content" parameterType="int" resultType="board.vo.BoardVO">
  		select * from board where num=#{value}
  	</select>
</mapper>